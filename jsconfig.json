{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "allowJs": true,
    "noEmit": true
  },
  "include": [
    "./**/*.js",
    "./Models/**/*.js",
    "./routes/**/*.js",
    "./utils/**/*.js"
  ],
  "exclude": [
    "node_modules",
    "client",
    "src"
  ]
}

